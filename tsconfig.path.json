{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components": ["src/components"],
      "@components/*": ["src/components/*"],
      "@templates": ["src/templates"],
      "@templates/*": ["src/templates/*"],
      "@pages": ["src/components/pages"],
      "@pages/*": ["src/components/pages/*"],
      "@utils": ["src/utils"],
      "@utils/*": ["src/utils/*"],
      "@api": ["src/api"],
      "@api/*": ["src/api/*"],
      "@hooks": ["src/hooks"],
      "@hooks/*": ["src/hooks/*"],
      "@definitions": ["src/definitions"],
      "@definitions/*": ["src/definitions/*"],
      "@navigation": ["src/navigation"],
      "@navigation/*": ["src/navigation/*"],
      "@providers": ["src/providers"],
      "@providers/*": ["src/providers/*"],
      "@generated": ["src/generated"],
      "@generated/*": ["src/generated/*"]
    }
  }
}
